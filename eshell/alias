alias rmds find . -name '*.dSYM' -type d -print | xargs -I '{}' rm -rf '{}'
alias fow find-file-other-window nil
alias ff find-file $1
alias dow dired-other-window $1
alias cbd cmake -DCMAKE_EXPORT_COMPILE_COMMANDS=1 -DCMAKE_BUILD_TYPE=Debug . $1
alias ll ls -al
alias cclean cmake --build . --target clean -j 4
alias cbuild cmake --build . -j 8
